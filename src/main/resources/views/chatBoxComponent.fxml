<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="chatBoxPane" maxWidth="183.0" nodeOrientation="LEFT_TO_RIGHT" onMouseClicked="#chatBoxOnClick" prefHeight="75.0" prefWidth="180.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChatBoxController">
   <children>
      <HBox alignment="CENTER" prefHeight="75.0" prefWidth="180.0" spacing="10.0">
         <children>
            <ImageView fx:id="userImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/female.png" />
               </image></ImageView>
            <Label fx:id="nameLabel" prefHeight="18.0" prefWidth="111.0" style="-fx-text-fill: #F5F5F5;" text="Shay">
               <font>
                  <Font name="Damascus Regular" size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="chatPaneBasic" />
      <String fx:value="chatPaneHover" />
   </styleClass>
</Pane>
